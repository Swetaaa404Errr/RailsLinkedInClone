<h1> Listing All Users </h1>

<table>
   <thead>
     <tr>
       <th>User Email</th>
     </tr>
    </thead>
    <tbody>


    <% @userl.each do |user| %>
    <tr>



    <td><%= user.username %></td>
    

     

         <td><%= link_to 'Delete', user_delete_path(user) , method: :delete , data: { confirm: 'Are you sure?'}%></td>

         </tr>
     
    <% end %>
    </tbody> 

   